{
   // Use IntelliSense to learn about possible attributes.
   // Hover to view descriptions of existing attributes.
   // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Clean Script",
         "type": "coreclr",
         "request": "launch",
         "program": "${workspaceFolder}/cs_tools/clean_script/bin/Debug/netcoreapp3.1/clean_script.exe",
         "preLaunchTask": "build_clean_script",
         "logging": {
            "moduleLoad": false
         }
      },
      {
         "name": "JS To Rpy",
         "type": "coreclr",
         "request": "launch",
         "program": "${workspaceFolder}/cs_tools/js2rpy/bin/Debug/netcoreapp3.1/js2rpy.exe",
         "cwd": "${workspaceFolder}/cs_tools/js2rpy",
         "preLaunchTask": "build_js2rpy",
         "logging": {
            "moduleLoad": false
         }
      },
      {
         "name": "Python Attach",
         "type": "python",
         "request": "attach",
         "port": 3000,
         "host": "localhost",
      },
      {
         "name": "Python Run",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "console": "internalConsole"
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Launch Game Mod",
         "program": "${workspaceFolder}\\index.js"
      },
   ]
}